<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: FFFFFF;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@../css/style.css" />
        <URL value="@../css/validations.css" />
    </stylesheets>
    <columnConstraints>
        <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="-Infinity" />
        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" valignment="CENTER" vgrow="ALWAYS" />
    </rowConstraints>
    <children>
        <VBox alignment="CENTER" style="-fx-background-color: FFFFFF;" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
            <children>
                <ImageView fitHeight="300.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                    <image>
                        <Image url="@../images/logoRPR.jpg" />
                    </image>
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </ImageView>
            </children>
        </VBox>
        <VBox alignment="CENTER" style="-fx-background-color: #1A1A1C;" GridPane.columnIndex="1">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" styleClass="heading" text="%SignupHeadingLabel" VBox.vgrow="ALWAYS" />
                <Label alignment="CENTER" styleClass="subheading" text="%SignupSubheadingLabel" />

                <HBox maxWidth="1.7976931348623157E308">
                    <children>
                        <Label styleClass="paragraph" text="%FirstName" textAlignment="CENTER" HBox.hgrow="ALWAYS">
                            <font>
                                <Font size="18.0" />
                            </font>
                            <padding>
                                <Insets bottom="5.0" top="5.0" />
                            </padding>
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="firstNameSignUpTextField" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </TextField>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox maxWidth="1.7976931348623157E308">
                    <children>
                        <Label styleClass="paragraph" text="%LastName" textAlignment="CENTER" HBox.hgrow="ALWAYS">
                            <font>
                                <Font size="18.0" />
                            </font>
                            <padding>
                                <Insets bottom="5.0" top="5.0" />
                            </padding>
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="lastNameSignUpTextField" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </TextField>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox maxWidth="1.7976931348623157E308">
                    <children>
                        <Label styleClass="paragraph" text="%Username" textAlignment="CENTER" HBox.hgrow="ALWAYS">
                            <font>
                                <Font size="18.0" />
                            </font>
                            <padding>
                                <Insets bottom="5.0" top="5.0" />
                            </padding>
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="usernameSignUpTextField" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </TextField>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox maxWidth="1.7976931348623157E308">
                    <children>
                        <Label styleClass="paragraph" text="%EmailAdress" textAlignment="CENTER" HBox.hgrow="ALWAYS">
                            <font>
                                <Font size="18.0" />
                            </font>
                            <padding>
                                <Insets bottom="5.0" top="5.0" />
                            </padding>
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="emailAdressSignUpTextField" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </TextField>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox maxWidth="1.7976931348623157E308">
                    <children>
                        <Label styleClass="paragraph" text="%Password" textAlignment="CENTER" HBox.hgrow="ALWAYS">
                            <font>
                                <Font size="18.0" />
                            </font>
                            <padding>
                                <Insets bottom="5.0" top="5.0" />
                            </padding>
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </Label>
                        <PasswordField fx:id="passwordSignUpPasswordField" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </PasswordField>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox maxWidth="1.7976931348623157E308">
                    <children>
                        <Label styleClass="paragraph" text="%RepeatPassword" textAlignment="CENTER" HBox.hgrow="ALWAYS">
                            <font>
                                <Font size="18.0" />
                            </font>
                            <padding>
                                <Insets bottom="5.0" top="5.0" />
                            </padding>
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </Label>
                        <PasswordField fx:id="repeatPasswordSignUpPasswordField" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </PasswordField>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <ButtonBar>
                    <buttons>
                        <Label alignment="CENTER_RIGHT" styleClass="paragraph" text="%SignupCreateAccountLabel" />
                        <Button defaultButton="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" styleClass="buttonframe" text="%SignUp" onAction="#signUp">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
                    </buttons>
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="70.0" top="10.0" />
                    </VBox.margin>
                </ButtonBar>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
    </children>
</GridPane>
